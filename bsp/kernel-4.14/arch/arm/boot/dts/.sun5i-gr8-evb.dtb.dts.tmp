   dma-names = "rx", "tx";
   status = "disabled";
  };

  sid: eeprom@01c23800 {
   compatible = "allwinner,sun4i-a10-sid";
   reg = <0x01c23800 0x10>;
  };

  rtp: rtp@01c25000 {
   compatible = "allwinner,sun5i-a13-ts";
   reg = <0x01c25000 0x100>;
   interrupts = <29>;
   #thermal-sensor-cells = <0>;
  };

  uart0: serial@01c28000 {
   compatible = "snps,dw-apb-uart";
   reg = <0x01c28000 0x400>;
   interrupts = <1>;
   reg-shift = <2>;
   reg-io-width = <4>;
   clocks = <&ccu 59>;
   status = "disabled";
  };

  uart1: serial@01c28400 {
   compatible = "snps,dw-apb-uart";
   reg = <0x01c28400 0x400>;
   interrupts = <2>;
   reg-shift = <2>;
   reg-io-width = <4>;
   clocks = <&ccu 60>;
   status = "disabled";
  };

  uart2: serial@01c28800 {
   compatible = "snps,dw-apb-uart";
   reg = <0x01c28800 0x400>;
   interrupts = <3>;
   reg-shift = <2>;
   reg-io-width = <4>;
   clocks = <&ccu 61>;
   status = "disabled";
  };

  uart3: serial@01c28c00 {
   compatible = "snps,dw-apb-uart";
   reg = <0x01c28c00 0x400>;
   interrupts = <4>;
   reg-shift = <2>;
   reg-io-width = <4>;
   clocks = <&ccu 62>;
   status = "disabled";
  };

  i2c0: i2c@01c2ac00 {
   compatible = "allwinner,sun4i-a10-i2c";
   reg = <0x01c2ac00 0x400>;
   interrupts = <7>;
   clocks = <&ccu 56>;
   status = "disabled";
   #address-cells = <1>;
   #size-cells = <0>;
  };

  i2c1: i2c@01c2b000 {
   compatible = "allwinner,sun4i-a10-i2c";
   reg = <0x01c2b000 0x400>;
   interrupts = <8>;
   clocks = <&ccu 57>;
   status = "disabled";
   #address-cells = <1>;
   #size-cells = <0>;
  };

  i2c2: i2c@01c2b400 {
   compatible = "allwinner,sun4i-a10-i2c";
   reg = <0x01c2b400 0x400>;
   interrupts = <9>;
   clocks = <&ccu 58>;
   status = "disabled";
   #address-cells = <1>;
   #size-cells = <0>;
  };

  timer@01c60000 {
   compatible = "allwinner,sun5i-a13-hstimer";
   reg = <0x01c60000 0x1000>;
   interrupts = <82>, <83>;
   clocks = <&ccu 40>;
  };

  fe0: display-frontend@01e00000 {
   compatible = "allwinner,sun5i-a13-display-frontend";
   reg = <0x01e00000 0x20000>;
   interrupts = <47>;
   clocks = <&ccu 89>, <&ccu 89>,
     <&ccu 84>;
   clock-names = "ahb", "mod",
          "ram";
   resets = <&ccu 4>;
   status = "disabled";

   ports {
    #address-cells = <1>;
    #size-cells = <0>;

    fe0_out: port@1 {
     #address-cells = <1>;
